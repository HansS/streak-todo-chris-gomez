<div class="container">
  {{#each alerts}}
  <div class="alert alert-{{type}} alert-dismissible" role="alert">
    <button type="button" class="close" can-click="dismissAlert">
      <span aria-hidden="true">&times;</span>
      <span class="sr-only">Close</span>
    </button>
    <strong>{{heading}}!</strong> {{message}}
  </div>
  {{/each}}
</div>